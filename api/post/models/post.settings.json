{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titulo": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Resumen": {
      "type": "text",
      "required": true
    },
    "Cuerpo": {
      "type": "richtext",
      "required": true
    },
    "Imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "categorias": {
      "collection": "categoria"
    },
    "servicio": {
      "via": "posts",
      "model": "servicio"
    }
  }
}
